{
    "executionRoleArn": "arn:aws:iam::544793848984:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "image": "544793848984.dkr.ecr.us-west-2.amazonaws.com/bmdc:lastest",
            "name": "bmdc",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "bmdc"
}
